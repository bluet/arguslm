# Backend Configuration
SECRET_KEY=your-secret-key-here-change-in-production
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1

# Database Configuration
# For PostgreSQL (recommended for production):
DATABASE_URL=postgresql+asyncpg://llm_user:llm_password@db:5432/llm_perf_mon
# For SQLite with aiosqlite (development only):
# DATABASE_URL=sqlite+aiosqlite:///./llm_perf_mon.db

# PostgreSQL Configuration (for docker-compose db service)
DB_USER=llm_user
DB_PASSWORD=llm_password
DB_NAME=llm_perf_mon

# LiteLLM Configuration
LITELLM_LOG_LEVEL=INFO

# Frontend Configuration
NODE_ENV=production
VITE_API_URL=http://localhost:8000

# Provider API Keys (add as needed)
# OPENAI_API_KEY=your-key-here
# ANTHROPIC_API_KEY=your-key-here
# GOOGLE_API_KEY=your-key-here
